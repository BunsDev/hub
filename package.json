{
  "name": "@web3hub/ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "lint": "yarn run eslint . --quiet",
    "lint:fix": "yarn run eslint . --fix",
    "build": "next build",
    "start": "next start",
    "export": "rm -rf .next out && next build && next export",
    "deploy": "cd out && surge && cd ..",
    "prepare": "husky install",
    "migrate:up": "NODE_TLS_REJECT_UNAUTHORIZED=0 npm exec ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "NODE_TLS_REJECT_UNAUTHORIZED=0 npm exec ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "dependencies": {
    "@3id/connect": "0.1.6",
    "@ceramicnetwork/3id-did-resolver": "1.4.0",
    "@ceramicnetwork/common": "1.3.0",
    "@ceramicnetwork/http-client": "1.2.0",
    "@ceramicstudio/idx": "0.12.1",
    "@ceramicstudio/idx-tools": "0.11.0",
    "@ensdomains/ensjs": "2.0.1",
    "@ethersproject/basex": "^5.4.0",
    "@monaco-editor/react": "^4.1.3",
    "@nodefactory/dappeteer": "^2.0.0",
    "@vercel/node": "^1.12.1",
    "@web3api/client-js": "0.0.1-prealpha.37",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.37",
    "@web3api/react": "0.0.1-prealpha.37",
    "animate.css": "^4.1.1",
    "axios": "0.21.1",
    "bnc-onboard": "latest",
    "cheerio": "^1.0.0-rc.6",
    "clean-html": "^1.5.0",
    "content-hash": "2.5.2",
    "cypress": "^7.4.0",
    "dids": "2.1.0",
    "ethers": "5.1.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "joi": "17.4.0",
    "js-yaml": "^4.1.0",
    "key-did-resolver": "1.2.0",
    "md5": "2.3.0",
    "next": "^10.0.5",
    "pg": "^8.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropdown-select": "^4.7.4",
    "react-html-parser": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "swr": "^0.5.3",
    "theme-ui": "^0.9.1",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^17.0.8",
    "@types/react-html-parser": "2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.2",
    "typescript": "^4.2.4",
    "husky": "^7.0.0"
  }
