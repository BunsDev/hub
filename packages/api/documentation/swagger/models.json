    "properties": {
      "id": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "subtext": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "icon": {
        "type": "string"
      },
      "locationUri": {
        "type": "string"
      },
      "pointerUris": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "ownerId": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "id",
      "description",
      "name",
      "subtext",
      "icon",
      "locationUri",
      "pointerUris"
    ]
  },
  "User": {
    "id": "UserData",
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "integer"
      },
      "username": {
        "type": "string"
      },
      "address": {
        "type": "string"
      },
      "accessToken": {
        "type": "string"
      },
      "authType": {
        "type": "integer"
      },
      "githubId": {
        "type": "string"
      }
    },
    "required": ["id"]
  },
  "Auth": {
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "authentication",
    "type": "object",
    "properties": {
      "github": {
        "type": "object",
        "properties": {
          "accessToken": {
            "$ref": "#/definitions/Jwe"
          }
        }
      }
    }
  },
  "definitions": {
    "Jwe": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "JWE",
      "type": "object",
      "properties": {
        "protected": { "type": "string" },
        "iv": { "type": "string" },
        "ciphertext": { "type": "string" },
        "tag": { "type": "string" },
        "aad": { "type": "string" },
        "recipients": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "header": {
                "type": "object",
                "properties": {
                  "alg": { "type": "string" },
                  "iv": { "type": "string" },
                  "tag": { "type": "string" },
                  "epk": { "type": "object" },
                  "kid": { "type": "string" }
                },
                "required": ["alg", "iv", "tag"]
              },
              "encrypted_key": { "type": "string" }
            },
            "required": ["header", "encrypted_key"]
          }
        }
      },
      "required": ["protected", "iv", "ciphertext", "tag"]
    }
  }
