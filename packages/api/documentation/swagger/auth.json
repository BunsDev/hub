{
  "/auth/sign-in": {
    "post": {
      "summary": "Log in using github or ethereum addess",
      "parameters": [
        {
          "in": "body",
          "description": "Address",
          "schema": {
            "type": "object",
            "properties": {
              "address": {
                "type": "string"
              }
            }
          }
        }
      ],
      "responses": {
        "200": {
          "schema": {
            "$ref": "#/definitions/User"
          }
        },
        "500": {
          "status": "500",
          "error": "Server error"
        }
      }
    }
  }
}
